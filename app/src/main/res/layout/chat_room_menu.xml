    <data>
        <import type="android.view.View" />
        <variable
            name="markAsReadClickListener"
            type="View.OnClickListener" />
        <variable
            name="callClickListener"
            type="View.OnClickListener" />
        <variable
            name="muteClickListener"
            type="View.OnClickListener" />
        <variable
            name="unMuteClickListener"
